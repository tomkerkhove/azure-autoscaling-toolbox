{
  "version": "2.0",
  "extensions": {
    "durableTask": {
      "hubName": "azureautoscalingtoolbox"
    },
    "http": {
      "routePrefix": "api",
      "customHeaders": {
        "Server": null
      }
    }
  },
    "logging": {
        "applicationInsights": {
            "samplingExcludedTypes": "Request",
            "samplingSettings": {
                "isEnabled": true
            }
        }
    }
}